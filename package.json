{
  "name": "web-interview",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=10.13"
  },
  "scripts": {
    "start": "react-scripts start & yarn server",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "server": "yarn json-server --watch ./data/data.json --port 3010"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma es5 --no-semi --write",
      "eslint --quiet",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "json-server": "^0.15.0",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "check-prop-types": "^1.1.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "husky": "^3.0.4",
    "jest-enzyme": "^7.1.0",
    "lint-staged": "^8.2.1",
    "react-scripts": "^3.0.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
